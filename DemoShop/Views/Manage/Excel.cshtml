@model DemoShop.ViewModels.ExcelViewModel
@using DemoShop.Infrastructure
@{
    ViewBag.Title = "Excel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="bg-info">
    <div class="row container_content">
        <div class="col-12 text-center UpSiteMapIconLogin"> <spam id="ManageMenuSiteMap">@Html.MvcSiteMap().SiteMapPath()</spam></div>
        <div class="col-12 UnderSiteMapIconLogin"><img alt="Obraz konta" src="@Url.Icons("icons8-microsoft-excel-100.png")" /></div>
    </div>
</section>
<section class="bg-white moreMargin" style="margin-top:50px;">

    <div class="row container_content">

        <div class="col-md-12 col-12 col-sm-12">


            <div class="panel">
                <div class="panel-heading">
                    <a href="@Url.Action("ExportToExcel")" class="btn btn-primary">Export</a>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th>Email</th>
                                <th>Kod wypisania</th>
                                <th>Adres do wypisania</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.Technologies)
                            {
                                <tr>
                                    <td>@item.Email</td>
                                    <td>@item.UnscribeCode</td>
                                    <td>@item.UnscribeLink</td>

                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>

        </div>



    </div>

</section>